    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "typeorm migration:create",
    "migration:generate": "typeorm migration:generate -d dist/database/data-source -p",
    "migration:run": "typeorm migration:run -d dist/database/data-source",
    "migration:revert": "typeorm migration:revert -d dist/database/data-source"
  },
